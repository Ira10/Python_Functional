    "n = int(input(\"Enter the interger\"))\n",
    "\n",
    "def cube_(p):\n",
    "    square = 0\n",
    "\n",
    "    for i in range(p+1):\n",
    "        square = square + i ** 3\n",
    "    return square\n",
    "\n",
    "print(cube_(n))"
